{% extends "::base.html.twig" %}

{% block body %}
    {% if form is defined %}
    <form action="" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}

        <input type="submit" />
    </form>
    {% endif %}

    {% if errors is defined %}
        {% for error in errors %}

        {% endfor %}
    {% endif %}
{% endblock %}